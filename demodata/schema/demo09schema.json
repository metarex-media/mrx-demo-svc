{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Metarex NAB Demo 09 schema",
	"description": "The Schema for an array of videos at varying levels of quality",
    "type": "array",
    "minItems": 1,
    "maxItems":10,
    "items": [
      {
        "type": "object",
        "properties": {
          "levelOfQuality": {
            "type": "integer"
          },
          "width": {
            "type": "integer"
          },
          "Height": {
            "type": "integer"
          },
          "fileSizeBytes": {
            "type": "integer"
          },
          "fileName": {
            "type": "string"
          }
        },
        "required": [
          "width",
          "Height",
          "fileName"
        ]
      }
    ]
  }